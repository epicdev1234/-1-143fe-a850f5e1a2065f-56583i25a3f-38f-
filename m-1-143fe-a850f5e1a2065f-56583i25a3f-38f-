-- Venyx UI Loadstring
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("ScriptX Hub v1.2", 5013109572)

-- Themes
local themes = {
Background = Color3.fromRGB(27, 12, 12),
Glow = Color3.fromRGB(255, 0, 0),
Accent = Color3.fromRGB(78, 19, 19),
LightContrast = Color3.fromRGB(57, 3, 3),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(197, 17, 17)
}

-- Tab's
local player = venyx:addPage("Player", 5012544693)
local credits = venyx:addPage("Credits", 5012544693)

-- Section's
local player3 = player:addSection("More features coming in v1.3 very soon")
local player1 = player:addSection("Player")
local player2 = player:addSection("Combat")
local credits1 = credits:addSection("Discord: https://discord.io/scriptxhub")
local credits2 = credits:addSection("Developers: thatoneboy#9999 & Hank#4242")
local credits3 = credits:addSection("Owners: thatoneboy#9999 & Hank#4242")

--Page 1, Section 1
player1:addButton("Reset", function()
    if game:GetService("Players").LocalPlayer.Character.Humanoid.Health > 0 then
        game:GetService("Players").LocalPlayer.Character.Humanoid.Health = 0
    end
end)

player1:addButton("Noclip", function()
        noclip = game:GetService("RunService").Stepped:Connect(function()
            for i, v in ipairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                if v:IsA("BasePart")then
                    v.CanCollide = false
                end
            end
        end)
end)

--Page 1, Section 2
player2:addButton("Aimbot", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HankRBXHelper/Aimbot-GUI/main/%5BArsenal%5D%20GUI.lua", true))()
    
    local CoreGUI = game:GetService("CoreGui")
    
    CoreGUI.ScreenGui:WaitForChild("Menu Frame").Visible = false
    CoreGUI.ScreenGui.Frame.Visible = true
    CoreGUI.ScreenGui.Frame.Draggable = true
    CoreGUI.ScreenGui.Frame.Titel.Text = "GUI"
    CoreGUI.ScreenGui.Frame.MainFrame.Frame2:WaitForChild("Bring all").Visible = false
    CoreGUI.ScreenGui.Frame.MainFrame.Frame2:WaitForChild("God mode").Visible = false
    CoreGUI.ScreenGui.Frame.MainFrame.Frame2:WaitForChild("Esp").Visible = false
    CoreGUI.ScreenGui.Frame.MainFrame.Frame2:WaitForChild("Speed").Visible = false
    CoreGUI.ScreenGui.Frame.MainFrame.Frame2:WaitForChild("Admin").Visible = false
    
    CoreGUI.ScreenGui.Frame.MainFrame.Frame2.Aimbot.MouseButton1Click:Connect(function()
        wait(3)
        CoreGUI.ScreenGui:Destroy()
    end)
end)

player2:addButton("Godmode", function()
    game.Players.LocalPlayer.Character.Health:Destroy()
end)

-- load
venyx:SelectPage(venyx.pages[1], true)
